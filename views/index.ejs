<%- include('includes/head.ejs'); %>
<%- include('includes/header.ejs'); %>
<main>
    <section>
        <% for (let i = 0; i < data.length; i++) { %>
        <article id="<%=data[i]._id%>">
            <a href="/<%=data[i]._id%>">
                <img src="uploads/<%=data[i].image%>" alt="<%=data[i].title%>" width="300">
                <h2><%= data[i].title %></h2>
            </a>
        </article>
        <% } %>
    </section>
</main>

<%- include('includes/footer.ejs'); %>